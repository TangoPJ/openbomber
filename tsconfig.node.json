{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "strict": true,
    "outDir": "./server-dist",
    "noEmit": false
  },
  "include": [
    "./server",
    "./src/types.ts",
    "./src/library/socketApi.ts",
    "./vite.config.ts",
    "./src/config.ts",
    "./src/proto.ts",
    "./src/library/Buffers.ts",
    "./src/library/copyBuffer.ts",
    "./src/library/isEqualBuffer.ts",
    "./src/library/makeEffect.ts"
  ]
}